# База данных (для Docker)
DATABASE_URL="postgresql://postgres:password@postgres:5432/auto_project"
POSTGRES_DB=auto_project
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# AWS S3 (используйте ваши реальные ключи)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=eu-north-1
AWS_S3_BUCKET=autodealer-images

# Next.js
NODE_ENV=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_for_local

# Дополнительные настройки
NEXT_PUBLIC_APP_URL=http://localhost:3000
